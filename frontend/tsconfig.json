{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "data/data.ts",
    "data/_products.ts",
    "src/slices/store.ts",
    "build/trash/ProductPage.test.tsx",
    "build/trash/basilisk.ts",
    "src/pages/IllustrationsPage/Filter.test.tsx",
    "src/utils/products.ts",
    "src/pages/ProductPage/ProductPage.test.tsx"
  ],
  "exclude": ["cypress.config.ts", "cypress", "node_modules"]
}
